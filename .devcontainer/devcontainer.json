{
  "name": "iSpeed (Record and retrieve internet speeds)",
  "dockerFile": "Dockerfile",
  "customizations": {
    "vscode": {
      "settings": {
        "dotfiles.repository": "https://github.com/losalamosal/dotfiles",
        "dotfiles.targetPath": "~/dotfiles",
        "dotfiles.installCommand": "install.sh",
        "terminal.integrated.defaultProfile.linux": "zsh",
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
        "cfnLint.path": "/home/devboy/.venv/bin/cfn-lint"
      },
      "extensions": [
        "kddejong.vscode-cfn-lint",
        "dannysteenman.cloudformation-yaml-snippets",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "mounts": [
    "source=${env:HOME}${env:USERPROFILE}/.aws,target=/home/devboy/.aws,type=bind",
    "source=${env:HOME}${env:USERPROFILE}/.vscode-zsh-hists/ispeed,target=/home/devboy/.zsh_history,type=bind"
  ],
  "remoteUser": "devboy"
}
